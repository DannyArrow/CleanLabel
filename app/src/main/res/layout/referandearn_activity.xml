<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout6"
    android:background="@color/whitee"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/top_bars"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <include
            layout="@layout/toolbar_fours"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageView
            android:padding="50dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/coin1"/>

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView

                android:gravity="center"
                android:id="@+id/txtview_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/imageView35"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"

                android:fontFamily="@font/sf_pro_display_medium"
                android:text="invite your friends and earn\n free credits"
                android:textColor="#222f3e"
                android:textSize="22sp"
                tools:text="invite your friends and earn\n free credits" />

            <TextView
                android:gravity="center"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/txtview_2"

                android:layout_marginStart="9.5dp"
                android:layout_marginLeft="9.5dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="13.5dp"
                android:layout_marginRight="13.5dp"
                android:fontFamily="@font/sf_pro_display_light"


                android:lineSpacingExtra="2sp"

                android:text="Give your friends 10 dollars credit on their first\n order,once they placed order you'll also get\n 10 dollars credit."
                android:textColor="#576574"
                android:textSize="14sp" />

        </LinearLayout>


        <LinearLayout
            android:padding="35dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:orientation="vertical"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">



                    <TextView
                        android:layout_marginLeft="10dp"


                        android:fontFamily="@font/sf_pro_display_medium"
                        android:textSize="9sp"
                        android:textColor="@color/color_2"


                        android:text="Instagram"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <RelativeLayout
                        android:id="@+id/one"
                        android:background="@drawable/social_bg1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_centerInParent="true"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/insta1"/>
                    </RelativeLayout>



                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginLeft="10dp"


                        android:fontFamily="@font/sf_pro_display_medium"
                        android:textSize="9sp"
                        android:textColor="@color/color_2"


                        android:text="Twitter"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <RelativeLayout
                        android:id="@+id/two"
                        android:background="@drawable/social_bg1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_centerInParent="true"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/twitter1"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginLeft="10dp"


                        android:fontFamily="@font/sf_pro_display_medium"
                        android:textSize="9sp"
                        android:textColor="@color/color_2"


                        android:text="Facebook"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <RelativeLayout
                        android:id="@+id/three"
                        android:background="@drawable/social_bg1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_centerInParent="true"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/fb1"/>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:orientation="vertical"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">



                    <TextView
                        android:layout_marginLeft="10dp"


                        android:fontFamily="@font/sf_pro_display_medium"
                        android:textSize="9sp"
                        android:textColor="@color/color_2"


                        android:text="line"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <RelativeLayout
                        android:id="@+id/four"
                        android:background="@drawable/social_bg1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_centerInParent="true"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/line1"/>
                    </RelativeLayout>



                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginLeft="10dp"


                        android:fontFamily="@font/sf_pro_display_medium"
                        android:textSize="9sp"
                        android:textColor="@color/color_2"


                        android:text="SMS Message"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <RelativeLayout
                        android:id="@+id/five"
                        android:background="@drawable/social_bg1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_centerInParent="true"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/msg1"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginLeft="10dp"


                        android:fontFamily="@font/sf_pro_display_medium"
                        android:textSize="9sp"
                        android:textColor="@color/color_2"


                        android:text="Whats app"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <RelativeLayout
                        android:id="@+id/six"
                        android:background="@drawable/social_bg1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_centerInParent="true"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/whtsapp1"/>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>