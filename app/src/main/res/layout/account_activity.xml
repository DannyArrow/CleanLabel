<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whitee"
    android:orientation="vertical">

    <include
        layout="@layout/toolbar_fours"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:layout_marginTop="15dp"
        android:gravity="fill_vertical"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/logo_above">


            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/sf_pro_display_medium"
                android:gravity="left"
                android:lineSpacingExtra="2sp"
                android:text="Account info"
                android:textColor="@color/color_1"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="Account info" />


        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView

        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:scaleType="fitXY"

        android:src="@drawable/line" />

    <LinearLayout
        android:padding="20dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:background="@drawable/ac_bg1"

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/profile"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ac_profile"/>

            <EditText


                android:textColorHint="@color/color_2"
                android:textColor="@color/color_1"

                android:textSize="14sp"
                android:paddingLeft="10dp"
                android:hint="Enter name"
                android:layout_centerVertical="true"
                android:background="@android:color/transparent"
                android:layout_toRightOf="@+id/profile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="15dp"
            android:background="@drawable/ac_bg1"

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/email"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ac_msg"/>

            <EditText


                android:textColorHint="@color/color_2"
                android:textColor="@color/color_1"

                android:textSize="14sp"
                android:paddingLeft="10dp"
                android:hint="Email"
                android:layout_centerVertical="true"
                android:background="@android:color/transparent"
                android:layout_toRightOf="@+id/email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="15dp"
            android:background="@drawable/ac_bg1"

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/phone"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ac_phone"/>

            <EditText
                android:inputType="phone"


                android:textColorHint="@color/color_2"
                android:textColor="@color/color_1"

                android:textSize="14sp"
                android:paddingLeft="10dp"
                android:hint="Enter phone number"
                android:layout_centerVertical="true"
                android:background="@android:color/transparent"
                android:layout_toRightOf="@+id/phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="15dp"
            android:background="@drawable/ac_bg1"

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/pass"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ac_lock"/>

            <EditText
                android:inputType="textPassword"


                android:textColorHint="@color/color_2"
                android:textColor="@color/color_1"

                android:textSize="14sp"
                android:paddingLeft="10dp"
                android:hint="Enter password"
                android:layout_centerVertical="true"
                android:background="@android:color/transparent"
                android:layout_toRightOf="@+id/pass"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="15dp"
            android:background="@drawable/ac_bg1"

            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/c_pass"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ac_lock"/>

            <EditText
                android:inputType="textPassword"

                android:textColorHint="@color/color_2"
                android:textColor="@color/color_1"

                android:textSize="14sp"
                android:paddingLeft="10dp"
                android:hint="Confirm password"
                android:layout_centerVertical="true"
                android:background="@android:color/transparent"
                android:layout_toRightOf="@+id/c_pass"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />

        </RelativeLayout>
    </LinearLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout68"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"



        android:gravity="fill_vertical"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/logo_above">


            <TextView
                android:id="@+id/dd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/sf_pro_display_medium"
                android:gravity="left"
                android:lineSpacingExtra="2sp"
                android:text="Display Info"
                android:textColor="@color/color_1"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="Display Info" />


        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView

        android:id="@+id/imageView165"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:scaleType="fitXY"

        android:src="@drawable/line" />
    <RelativeLayout
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"

        android:background="@drawable/ac_bg1"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/profile5"
            android:layout_marginLeft="10dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ac_home"/>

        <EditText


            android:textColorHint="@color/color_2"
            android:textColor="@color/color_1"

            android:textSize="14sp"
            android:paddingLeft="10dp"
            android:hint="Enter Address name"
            android:layout_centerVertical="true"
            android:background="@android:color/transparent"
            android:layout_toRightOf="@+id/profile5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />



    </RelativeLayout>

    <RelativeLayout
        android:layout_gravity="right"
        android:gravity="right"
        android:background="@drawable/ac_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginRight="60dp"
            android:gravity="center"
            android:layout_marginTop="34dp"
            android:fontFamily="@font/sf_pro_display_medium"
            android:letterSpacing="0.03"
            android:lineSpacingExtra="6sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Save"
            android:textSize="14sp"
            android:textColor="@color/whitee"
            />
    </RelativeLayout>

</LinearLayout>

